secret_number=9
#i= number of guesses user makes
guess_count=0
guess_limit =3
while guess_count < guess_limit:
 guess=int(input('guess: '))
 guess_count+=1
 if guess== secret_number:
   print('you won')
   break
 elif guess_count<guess_limit:
   print('another try')
else:
  print('sorry you are failed')
   